cantoXX = \relative c'' {
    \fourTwoCutTime
    \clef "petrucci-g"
    \key f \major

    R\breve | r2 a cs cs | d1 a2 d | }

altoXX = \relative c' {
    \fourTwoCutTime
    \clef "petrucci-c2"
    \key f \major

    R\breve*3 }

tenoreXX = \relative c' {
    \fourTwoCutTime
    \clef "petrucci-c3"
    \key f \major

    R\breve | d1 d2 d | f1 e }

bassoXX = \relative c' {
    \fourTwoCutTime
    \clef "petrucci-f3"
    \key f \major

    a1 a2 a | bf\breve | a\breve }

